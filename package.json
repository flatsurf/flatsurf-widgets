{
  "name": "flatsurf-widgets",
  "version": "0.1.0",
  "description": "Jupyter Widgets for flatsurf",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "homepage": "https://github.com/flatsurf/flatsurf-widgets",
  "bugs": {
    "url": "https://github.com/flatsurf/flatsurf-widgets/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Julian RÃ¼th",
    "email": "julian.rueth@fsfe.org"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/flatsurf/flatsurf-widgets"
  },
  "scripts": {
    "build:labextension": "npm run clean:labextension && mkdirp flatsurf_widgets/labextension && cd flatsurf_widgets/labextension && npm pack ../..",
    "build:lib": "webpack -p",
    "build:nbextension": "npm run build:lib",
    "build:all": "npm run build:labextension && npm run build:nbextension",
    "clean": "npm run clean:lib && npm run clean:nbextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf flatsurf_widgets/labextension",
    "clean:nbextension": "rimraf flatsurf_widgets/nbextension/static/index.js",
    "prepack": "npm run build:lib",
    "test": "npm run test:firefox",
    "test:chrome": "karma start --browsers=Chrome tests/karma.conf.js",
    "test:debug": "karma start --browsers=Chrome --singleRun=false --debug=true tests/karma.conf.js",
    "test:firefox": "karma start --browsers=Firefox tests/karma.conf.js",
    "test:ie": "karma start --browsers=IE tests/karma.conf.js",
    "watch": "npm-run-all -p watch:*",
    "watch:nbextension": "webpack --watch"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.10"
  },
  "devDependencies": {
    "@flatten-js/core": "^1.0.15",
    "@phosphor/application": "^1.6.0",
    "@phosphor/widgets": "^1.6.0",
    "@types/expect.js": "^0.3.29",
    "@types/gsap": "^1.20.2",
    "@types/lodash-es": "^4.17.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^11.13.8",
    "@types/webpack-env": "^1.13.6",
    "css-element-queries": "^1.1.1",
    "css-loader": "^2.1.1",
    "d3": "^5.9.2",
    "expect.js": "^0.3.1",
    "fs-extra": "^7.0.0",
    "gsap": "^2.1.2",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^3.0.13",
    "lodash-es": "^4.17.11",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.3",
    "octicons": "^8.5.0",
    "pan-zoom": "^3.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.2",
    "ts-loader": "^5.2.1",
    "typescript": "~3.1.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^8.1.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.1"
  },
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
